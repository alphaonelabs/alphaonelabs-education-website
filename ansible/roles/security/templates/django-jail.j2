[django-auth]
enabled = true
port = http,https
filter = django-auth
logpath = {{ project_path }}/logs/django.log
maxretry = 5
bantime = 3600
findtime = 600

[nginx-http-auth]
enabled = true
port = http,https
filter = nginx-http-auth
logpath = /var/log/nginx/error.log
maxretry = 5
bantime = 3600
findtime = 600 